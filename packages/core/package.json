{
  "name": "@ddb-lib/core",
  "version": "0.1.0",
  "description": "Core utilities for DynamoDB patterns and best practices",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
